<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.petworld_madebysocialworld.MapActivity" android:id="@+id/coordinatorLayout">


    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Soc un Mapa"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/textView" android:textSize="36sp"/>
    <Button
            android:text="LogIn"
            android:layout_width="143dp"
            android:layout_height="wrap_content"
            android:id="@+id/button2"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.501" android:layout_marginStart="48dp" android:onClick="goToLogIn"/>
    <Button
            android:text="Perfil"
            android:layout_width="126dp"
            android:layout_height="wrap_content"
            android:id="@+id/button3" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toEndOf="@+id/button2"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.371"
            app:layout_constraintVertical_bias="0.501" android:onClick="goToUserProfile"/>

</android.support.constraint.ConstraintLayout>